<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="XYButton">
    </declare-styleable>

    <declare-styleable name="XYSignalBars">
        <attr name="maxBars" format="integer"/>
    </declare-styleable>

    <declare-styleable name="XYEditView">
        <attr name="readOnly" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="XYSpinner">
        <attr name="values" format="reference"/>
    </declare-styleable>

    <declare-styleable name="XYPolygon">
        <attr name="sides" format="integer" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_width" format="integer" />
        <attr name="fill_color" format="color" />
        <attr name="fill_alpha" format="color" />
        <attr name="start_angle" format="float" />
        <attr name="inscribed_circle" format="boolean" />
        <attr name="fill_percent" format="float" />
        <attr name="fill_bitmap" format="integer" />
    </declare-styleable>

</resources>
